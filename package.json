{
  "name": "cv",
  "description": "CV using Gatsby",
  "version": "2.0.0",
  "author": "Scott Spence <yo@scottspence.dev>",
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby build",
    "dev": "gatsby develop -p 8080 -o",
    "format": "pretty-quick",
    "serve": "gatsby serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "printWidth": 70,
    "arrowParens": "avoid",
    "proseWrap": "always"
  },
  "dependencies": {
    "@mdx-js/mdx": "1.6.6",
    "@mdx-js/react": "1.6.6",
    "@theme-ui/presets": "^0.3.0",
    "date-fns": "2.14.0",
    "gatsby": "2.24.1",
    "gatsby-plugin-manifest": "2.4.18",
    "gatsby-plugin-mdx": "1.2.25",
    "gatsby-plugin-offline": "3.2.17",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-robots-txt": "1.5.1",
    "gatsby-plugin-sitemap": "2.4.11",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-seo-component": "2.0.2",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "husky": "4.2.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1"
  },
  "keywords": [
    "gatsby",
    "cv"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/cv"
  },
  "bugs": {
    "url": "https://github.com/spences10/cv/issues"
  },
  "homepage": "https://github.com/spences10/cv#readme"
}
