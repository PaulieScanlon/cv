{
  "name": "cv",
  "description": "CV using Gatsby",
  "version": "1.0.0",
  "author": "Scott Spence <spences10apps@gmail.com>",
  "dependencies": {
    "axios": "0.18.0",
    "dayjs": "1.5.18",
    "gatsby": "^1.9.247",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-react-next": "1.0.11",
    "react-helmet": "^5.2.0",
    "styled-components": "3.2.6"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-plugin-graphql": "2.1.1",
    "eslint-plugin-react": "7.7.0",
    "husky": "0.14.3",
    "prettier": "^1.12.0",
    "pretty-quick": "1.4.1"
  },
  "keywords": [
    "gatsby",
    "cv"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache/ && rm -rf cv/ && rm -rf public/",
    "build": "gatsby build && mv public cv && npm run wellknown",
    "dev": "gatsby develop",
    "format": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "test": "echo \"Error: no test specified\" && exit 1",
    "loadCv": "now src/cvjson/ && npm run aliasIt",
    "aliasIt": "now alias",
    "clearStale": "now rm cvjson --safe --yes",
    "wellknown": "cp -r .well-known/ cv/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/cv"
  },
  "bugs": {
    "url": "https://github.com/spences10/cv/issues"
  },
  "homepage": "https://github.com/spences10/cv#readme"
}
