{
  "name": "cv",
  "description": "CV using Gatsby",
  "version": "2.0.0",
  "author": "Scott Spence <yo@scottspence.dev>",
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby build",
    "dev": "gatsby develop -p 8080 -o",
    "format": "pretty-quick",
    "serve": "gatsby serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "printWidth": 70,
    "arrowParens": "avoid",
    "proseWrap": "always"
  },
  "dependencies": {
    "@callstack/react-theme-provider": "^2.1.0",
    "@mdx-js/mdx": "1.6.1",
    "@mdx-js/react": "1.6.1",
    "date-fns": "2.12.0",
    "gatsby": "2.20.36",
    "gatsby-image": "2.3.5",
    "gatsby-plugin-linaria": "^2.0.0",
    "gatsby-plugin-manifest": "2.3.7",
    "gatsby-plugin-mdx": "1.2.6",
    "gatsby-plugin-offline": "3.1.5",
    "gatsby-plugin-react-helmet": "3.2.5",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sharp": "2.5.7",
    "gatsby-plugin-sitemap": "2.4.2",
    "gatsby-plugin-zeit-now": "0.3.0",
    "gatsby-transformer-sharp": "2.4.7",
    "linaria": "^2.0.0-alpha.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-seo-component": "2.0.0"
  },
  "devDependencies": {
    "husky": "4.2.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1"
  },
  "keywords": [
    "gatsby",
    "cv"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/cv"
  },
  "bugs": {
    "url": "https://github.com/spences10/cv/issues"
  },
  "homepage": "https://github.com/spences10/cv#readme"
}
