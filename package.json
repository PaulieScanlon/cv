{
  "name": "online-cv",
  "description": "Online CV using Gatsby",
  "version": "1.0.0",
  "author": "Scott Spence <spences10apps@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.247",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "react-helmet": "^5.2.0"
  },
  "keywords": ["gatsby", "cv"],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "loadCv": "now src/cvjson/ && npm run aliasIt",
    "aliasIt": "now alias",
    "clearStale": "now rm cvjson --safe --yes"
  },
  "devDependencies": {
    "prettier": "^1.12.0"
  }
}
